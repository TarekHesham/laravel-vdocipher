{
  "name": "elfarmawy/vdocipher",
  "description": "Laravel package for VdoCipher video platform integration",
  "type": "library",
  "license": "MIT",
  "version": "1.1.0",
  "keywords": [
    "elfarmawy",
    "laravel",
    "vdocipher",
    "vdocipher-api",
    "streaming",
    "lms"
  ],
  "authors": [
    {
      "name": "Tarek Hesham",
      "email": "tarekelfarmawy@outlook.com",
      "homepage": "https://tarekelfarmawy.vercel.app",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/tarekhesham/laravel-vdocipher",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "ElFarmawy\\VdoCipher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ElFarmawy\\VdoCipher\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ElFarmawy\\VdoCipher\\Providers\\VdoCipherServiceProvider"
      ],
      "aliases": {
        "VdoCipher": "ElFarmawy\\VdoCipher\\Facades\\VdoCipher"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "support": {
    "email": "tarekelfarmawy@outlook.com",
    "issues": "https://github.com/tarekhesham/laravel-vdocipher/issues",
    "source": "https://github.com/tarekhesham/laravel-vdocipher"
  }
}
